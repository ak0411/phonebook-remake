{
  "extends": "airbnb",
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "max-len": [1, 200],
    "no-shadow": 0,
    "no-plusplus": 0,
    "import/no-unresolved": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": [
      1,
      {
        "skipUndeclared": true
      }
    ],
    "react/function-component-definition": 0,
    "import/no-import-module-exports": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "no-unused-vars": 0,
    "no-alert": 0
  }
}